//animate
int xx=40,yy=0,tt=0;

#define ROW_NUM     4 // four rows
#define COLUMN_NUM  4 // four columns

constexpr char WIFI_SSID[] = "Pooh";

int32_t getWiFiChannel(const char *ssid) {

    if (int32_t n = WiFi.scanNetworks()) {
        for (uint8_t i=0; i<n; i++) {
            if (!strcmp(ssid, WiFi.SSID(i).c_str())) {
                return WiFi.channel(i);
            }
        }
    }

    return 0;
}

//keypad
char keys[ROW_NUM][COLUMN_NUM] = {
  {'1', '2', '3', 'A'},
  {'4', '5', '6', 'B'},
  {'7', '8', '9', 'C'},
  {'*', '0', '#', 'D'}
};

byte pin_rows[ROW_NUM]      = {18, 5, 17, 16}; // GIOP19, GIOP18, GIOP5, GIOP17 connect to the row pins
byte pin_column[COLUMN_NUM] = {4, 0, 2, 15};   // GIOP16, GIOP4, GIOP0, GIOP2 connect to the column pins

Keypad keypad = Keypad( makeKeymap(keys), pin_rows, pin_column, ROW_NUM, COLUMN_NUM );

//  mac 
 uint8_t AddressServer[] = {0x3C, 0x61, 0x05, 0x03, 0xB5, 0xC0};// mac address

//Global variable
int playerID = 1;
char server_state = 'D'; 	                        //server
int rounds = 1; 			                            //
int input= -1;
char player_active[5] = {'0','0','0','0','0'} ;	  // (0 , 1 )
int player_score[5] = {0,0,0,0,0};		            // (-10  0)
int player_input;                                 //
char w_round[5] = {'0','0','0','0','0'};		      // (0 , 1 )
char w_game;		                                  //
long unsigned time_round;                         //
long unsigned time_stamp;
long unsigned timeConnect;
char ins_server_state = '/';
int checkD = 0;

typedef struct struct_message { //  struct
  char m_request;
  char m_server_state;
  char m_rounds;
  char m_player_active[5] = {'0','0','0','0','0'};
  int m_player_score[5] = {0,0,0,0,0};
  char m_w_round[5] = {'0','0','0','0','0'};
  char w_game;
  long unsigned m_time_round;
  int m_input;
  int m_playerID;
} struct_message;

struct_message myData; // 

void checkConnect(){
  if(server_state == 'I' || server_state =='Z'){
    if(millis()-timeConnect > 30500){
      server_state = 'D';
    }
  }else if(server_state == 'W'){
    if(millis()-timeConnect > 16500){
      server_state = 'D';
    }   
  }else if(server_state == 'S'){
    if(millis()-timeConnect > 4500){
      server_state = 'D';
    }   
  }else if(server_state != 'M' && server_state != 'E'){
    if(millis()-timeConnect > 8500){
      server_state = 'D';
    }
  }
}

void checkKey(){
  char key = keypad.getKey();
  if(key){
    if(key== 'C'){
      myData.m_request = 'C';
      myData.m_playerID = playerID;
      esp_err_t result = esp_now_send(AddressServer, (uint8_t *) &myData, sizeof(myData));
    }
    if(server_state=='I'){
      if(key - '0' < 10 ){
        int check = input;
        if (input==-1 || input==0){
          input = key-'0';
        }else if(input==10 && key=='0'){
          input = 100;
        }else{
          input = input*10 + (key-'0');
        }
        if(input>100){
          input = check;
        }
        Serial.print("input : ");
        Serial.println(input);
      }
      if(key == 'B'){
        input = -1;
      }
      if(key == 'A'){
        myData.m_request = 'A';
        myData.m_playerID = playerID;
        myData.m_input = input;
        esp_err_t result = esp_now_send(AddressServer, (uint8_t *) &myData, sizeof(myData));
        server_state = 'Z';
      }
    }
    if(server_state=='M' && key=='D'){
      myData.m_request = 'D';
      myData.m_playerID = playerID;
      checkD = 1;
      esp_err_t result = esp_now_send(AddressServer, (uint8_t *) &myData, sizeof(myData));
    }
  }
}

void DisplayServerstate(){
  if(server_state!=ins_server_state){
    ins_server_state = server_state;
    if(server_state == 'D'){
      Serial.println("Disconnected_state");
    }else if(server_state == 'M'){
      Serial.println("Main_state");
    }else if(server_state == 'S'){
      Serial.println("DisplayScore_state");
    }else if(server_state == 'W'){
      Serial.println("DisplayWinnerRound_state");
    }else if(server_state == 'R'){
      Serial.println("DisplayRule_state");
    }else if(server_state == 'G'){
      Serial.println("DisplayWinnerGame_state");
    }else if(server_state == 'L'){
      Serial.println("DisplayLoserGame_state");
    }else if(server_state == 'I'){
      Serial.println("WaitInput_state");
    }else if(server_state == 'F'){
      Serial.println("Failed_state");
    }else if(server_state == 'Z'){
      Serial.println("Submitted_state");
    }else if(server_state == 'U'){
      Serial.println("ServerUnreachable");
    }else if(server_state == 'E'){
      Serial.println("Ready_state");
    }
  }
}

//
void OnDataSent(const uint8_t *mac_addr, esp_now_send_status_t status) {
  Serial.print("\r\nLast Packet Send Status:\t");
  Serial.println(status == ESP_NOW_SEND_SUCCESS ? "Delivery Success" : "Delivery Fail");
  if(status!=ESP_NOW_SEND_SUCCESS && server_state=='I'){
    server_state = 'F';
  }else if(status!=ESP_NOW_SEND_SUCCESS){
    server_state = 'U';
  }
  if(status==ESP_NOW_SEND_SUCCESS && checkD == 1){
    server_state = 'E';
  }
  checkD=0;
}

void OnDataRecv(const uint8_t * mac, const uint8_t *incomingData, int len) {
  memcpy(&myData, incomingData, sizeof(myData));
  Serial.print("receive Server state : ");
  timeConnect = millis();
  Serial.println(myData.m_server_state);
  for(int i =0;i<5;i++){
    player_active[i] = myData.m_player_active[i];
  }
  if(player_active[playerID]=='0' && myData.m_server_state != 'M'){
    server_state = 'B';
  }else if(myData.m_server_state == 'S'){
    for(int i = 0 ;i <5 ;i++){
      player_score[i] = myData.m_player_score[i];
    }
    server_state = 'S';
  }else if(myData.m_server_state=='I'){
    rounds = myData.m_rounds;
    time_round = myData.m_time_round;

    time_stamp = millis();
    input = -1;
    server_state = 'I';

  }else if(myData.m_server_state == 'W'){
    for(int i = 0 ;i <5 ;i++){
      w_round[i] = myData.m_w_round[i];
    }
    server_state = 'W';
  }else if(myData.m_server_state == 'L'){
    for(int i = 0 ;i <5 ;i++){
      player_score[i] = myData.m_player_score[i];
    }
    server_state = 'L';
  }else if(myData.m_server_state == 'M' && player_active[playerID]=='1'){
    server_state = 'E';
  }else{
    server_state = myData.m_server_state;
  }
}

void setup() {
  Serial.begin(115200);

  //oled
  if(!display.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;); // Don't proceed, loop forever
  }
  delay(2000); // Pause for 2 seconds
  display.clearDisplay();

  // Wi-Fi Station
  WiFi.mode(WIFI_STA);

  int32_t channel = getWiFiChannel(WIFI_SSID);
  // WiFi.printDiag(Serial);
  esp_wifi_set_promiscuous(true);
  esp_wifi_set_channel(channel, WIFI_SECOND_CHAN_NONE);
  esp_wifi_set_promiscuous(false);
  // WiFi.printDiag(Serial);

  //  ESP-NOW
  if (esp_now_init() != ESP_OK) {
    Serial.println("Error initializing ESP-NOW");
    return;
  }

  //   OnDataRecv 
  esp_now_register_recv_cb(OnDataRecv);
  // OnDataSend 
  esp_now_register_send_cb(OnDataSent);

  esp_now_peer_info_t peerInfo;
  peerInfo.ifidx   = ESP_IF_WIFI_STA;
  peerInfo.encrypt = false;

  // 
  memcpy(peerInfo.peer_addr, AddressServer, 6); //  1
  if (esp_now_add_peer(&peerInfo) != ESP_OK) {
    Serial.println("Failed to add peer");
    return;
  }
}

//Function State
void Disconnected_state(){
  display.clearDisplay();
      display.setTextSize(1);
      display.setTextColor(WHITE);
      display.setCursor(48,32); 
      display.println("Disconnected");
      display.display();
}

void ServerUnreachable_state(){
  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(44,20);
  display.print("Sever ");
  display.print("\n    Unreachable ");
  display.display();
  delay(2000);
  server_state = 'D';
}

void Main_state(){
  display.clearDisplay();
  display.drawBitmap(
    (display.width() - LOGO_WIDTH) / 2,
    (display.height() - LOGO_HEIGHT) / 2,
    logo_logo,LOGO_WIDTH,LOGO_HEIGHT,1
  );
  display.display();
}

void DisplayScore_state(){
  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(30,32);
  display.print("Your score: ");
  display.print(player_score[playerID]);
  display.display();
}

void DisplayWinnerRound_state(){
  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(28,28);          
  if(w_round[playerID]=='1'){
    display.println("You Win\n     This Round");  
    display.display();
  }
  else{
    display.setTextSize(1);
    display.setTextColor(WHITE);
    display.setCursor(40,28);  
    display.println("Winner ");
    display.print("    Player:");
    for(int i=0;i<5;i++){
      if(w_round[i]=='1'){
        display.print(i);
        display.print(" ");
      }
    }
    display.display();
  }
}

void DisplayRule_state(){
  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(21,28);  
  display.print("Remain player: ");
  int amount_player = 0;
  for(int i=0;i<5;i++){
    if(player_score[i]!=-10){
      amount_player++;
    }
  }
  display.print(amount_player);
  display.display();
}

