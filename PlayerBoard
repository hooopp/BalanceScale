        delay(tt);

        display.clearDisplay();
        display.drawBitmap(xx, yy,heartbeat47,48,48, 1);
        display.display();
        delay(tt);

        display.clearDisplay();
        display.drawBitmap(xx, yy,heartbeat48,48,48, 1);
        display.display();
        delay(tt);

        display.clearDisplay();
        display.drawBitmap(xx, yy,heartbeat49,48,48, 1);
        display.display();
        delay(tt);
      }
}

void Failed_state(){
  display.clearDisplay();
      display.setTextSize(1);
      display.setTextColor(WHITE);
      display.setCursor(20,28); 
      display.println("Please submit\nagain");
      display.display();
  delay(2000);
  server_state = 'I';
}

void WaitingInput_state(){
  long unsigned int ins_time = time_round-(millis()-time_stamp);
  if(ins_time < 0){
    ins_time = 0;
  }
  display.clearDisplay();
      display.setTextSize(1);
      display.setTextColor(WHITE);
      display.setCursor(20,28); 
      display.print("Input: ");
      if(input != -1){
        display.print(input);
      }
      display.setTextSize(1);
      display.setTextColor(WHITE);
      display.setCursor(0,42);
      display.print("\nTime left ");
      display.print(ins_time/1000);
      display.print(" sec");
      display.display();
}

void Submitted_state(){
  display.clearDisplay();
      display.setTextSize(1);
      display.setTextColor(WHITE);
      display.setCursor(48,32); 
      display.println("Submitted");
      display.display();
}

void Ready_state(){
  display.clearDisplay();
      display.setTextSize(1);
      display.setTextColor(WHITE);
      display.setCursor(48,32); 
      display.println("Ready");
      display.display();
}

void loop() {
  checkKey();
  checkConnect();
  if(server_state == 'I'){
    WaitingInput_state();
  }else if(server_state == 'D'){
    Disconnected_state();
  }else if(server_state == 'U'){
    ServerUnreachable_state();
  }else if(server_state == 'M'){
    Main_state();
  }else if(server_state == 'S'){
    DisplayScore_state();
  }else if(server_state == 'W'){
    DisplayWinnerRound_state();
  }else if(server_state == 'R'){
    DisplayRule_state();
  }else if(server_state == 'G'){
    DisplayWinnerGame_state();
  }else if(server_state == 'L'){
    DisplayLoserGame_state();
  }else if(server_state == 'F'){
    Failed_state();
  }else if(server_state == 'Z'){
    Submitted_state();
  }else if(server_state== 'E'){
    Ready_state();
  }
}
